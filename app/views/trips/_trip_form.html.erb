<%= f.error_messages %></td>

<table class="trip_form">
  <tr>
    <td>
      <p>
        <%= f.label 'Date' %><%= f.date_select :date %>
      </p>
      <p>
        <%= f.label 'Departure Time' %>
        <%= f.select :depart, time_selectors, {:selected=>time_as_string(@trip.depart)} %>
      </p>
      <p>
        <%= f.label 'Return Time' %>
        <%= f.select :return, time_selectors, {:selected=>time_as_string(@trip.return)} %>
      </p>
      <p>
        <%= f.label :destination %>
        <%= f.text_field :destination %>
      </p>
      <p>
        <%= f.label :travelers %>
        <%= f.select(:travelers, (1..20)) %>
      </p>
      <p>
        <%= f.label :contact %>
        <%= f.text_field :contact %>
      </p>
    </td>
    <td>
      <div class="radio"
           <p>
          <%= f.label :community %><br />
          <%= f.radio_button( "community", 'Dancing Rabbit') %>Dancing Rabbit <br />
          <%= f.radio_button( "community", 'Sandhill') %>Sandhill <br />
          <%= f.radio_button( "community", 'Red Earth Farms') %>Red Earth Farms <br />
        </p>
        <p>
          <%#= f.label :preferred_vehicle %><br />
          <%# Vehicle.find(:all, :order=>"display_order ASC").each do |v| %>
            <%#= f.radio_button("preferred_vehicle", "#{v.name}")%>
            <%#= v.name %>
            <!-- <br /> -->
          <%# end %>
        </p>
      </div>
    </td>
  </tr>
  <tr>
    <td colspan="2">
      <p>
        <%= f.label :notes %><br />
        <%= f.text_area :notes, :cols=>60, :rows=>8%>
      </p>
    </td>
  </tr>
</table>